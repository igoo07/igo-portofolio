{
    "version": 2,
    "functions": {
      "api/index.php": { "runtime": "vercel-php@0.7.2" }
    },
    "routes": [
      { "src": "/build/(.*)", "dest": "/public/build/$1" },
      { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
      { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
      { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "SESSION_DRIVER": "cookie",
      "LOG_CHANNEL": "stderr"
    }
  }