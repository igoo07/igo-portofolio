{
    "version": 2,
    "functions": {
      "api/index.php": { "runtime": "vercel-php@0.7.2" }
    },
    "routes": [
      { "src": "/(css|js|images|assets)/(.*)", "dest": "/public/$1/$2" },
      { "src": "/(.*)", "dest": "/api/index.php" }
    ],
   "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "DB_CONNECTION": "sqlite",
    "DB_DATABASE": "/var/task/user/database/database.sqlite",
    "SESSION_DRIVER": "cookie",
    "LOG_CHANNEL": "stderr"
}
  }